
cd /data/norman/tmp/
wget https://ftp.ncbi.nlm.nih.gov/genomes/all/GCF/003/713/225/GCF_003713225.1_Cara_1.0/GCF_003713225.1_Cara_1.0_genomic.fna.gz
wget https://ftp.ncbi.nlm.nih.gov/genomes/all/GCF/003/713/225/GCF_003713225.1_Cara_1.0/GCF_003713225.1_Cara_1.0_protein.faa.gz
wget https://ftp.ncbi.nlm.nih.gov/genomes/all/GCF/003/713/225/GCF_003713225.1_Cara_1.0/GCF_003713225.1_Cara_1.0_genomic.gtf.gz
wget https://ftp.ncbi.nlm.nih.gov/genomes/all/GCF/003/713/225/GCF_003713225.1_Cara_1.0/GCF_003713225.1_Cara_1.0_genomic.gff.gz


mkdir /data/norman/coffee/Analysis_2020_pilot/dna-proto-workflow/genomes_and_annotations/snpeffdata/GCF_003713225.1_Cara_1.0_test

mv GCF_003713225.1_Cara_1.0_genomic.fna.gz /data/norman/coffee/Analysis_2020_pilot/dna-proto-workflow/genomes_and_annotations/snpeffdata/GCF_003713225.1_Cara_1.0_test/sequences.fa.gz
mv GCF_003713225.1_Cara_1.0_genomic.gtf.gz /data/norman/coffee/Analysis_2020_pilot/dna-proto-workflow/genomes_and_annotations/snpeffdata/GCF_003713225.1_Cara_1.0_test/genes.gtf.gz
mv GCF_003713225.1_Cara_1.0_protein.faa.gz /data/norman/coffee/Analysis_2020_pilot/dna-proto-workflow/genomes_and_annotations/snpeffdata/GCF_003713225.1_Cara_1.0_test/protein.fa.gz

cd /data/norman/coffee/Analysis_2020_pilot/dna-proto-workflow/

snpEff build -gtf22 -v GCF_003713225.1_Cara_1.0_test
snpEff dump GCF_003713225.1_Cara_1.0_test

snpEff build -gff3 -v GCF_003713225.1_Cara_1.0_test
snpEff dump GCF_003713225.1_Cara_1.0_test

#in my snpEff.config
